    "devDependencies": {
        "@types/node": "^20.10.0",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "typescript": "^5.3.3",
        "vitest": "^4.0.16"
    },
    "dependencies": {
        "@types/express": "^4.17.21",
        "express": "^4.18.2"
    }
}
